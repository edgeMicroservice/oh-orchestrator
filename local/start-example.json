{
	"SERVER_PORT": 8022,
	// "SERVER_ID": "mDS-12345",
	// "DEBUG": "swagger-tools:middleware.*",
	// -- logs
	"LOG_LEVEL": "debug",
	"CONSOLE_LEVEL": "silly",
	"NO_STACK": "yes",
	"LOG_MODE": "none",
	// "LOG_MODE": "all",
	// "LOG_MODE": "awsS3",
	// "standAlone": "yes",
	// -- providers
	// "LOCATION_PROVIDER": "http://ip-api.com/json",
	// "IP_LOCATION_PROVIDER": "http://ip_api.com/json",
	// "LOCATION_PROVIDER_KEY": "---a key---",
	// "CLOUD_PROVIDER": "AWS",
	// -- security
	"MST_SET": "off",
	"oauthImplicitNeeded": "no",
	"mIDNeeded": "no",
	"API_KEYS": "abcd",
	// "SERVER_SECURITY_SET": "off",
	// -- dependencies, the audiences are generated by mST
	"EDGEDAEMON_URL": "http://localhost:8021/edgedaemon/v1",
	"MESS_APIKEY": "--mess-key--",
	"MDEPLOY_APIKEY": "--mdeploy-key--",
	"EDGEDAEMON_APIKEY": "--edge-daemon-key--",
	// -- environment
	// Custom
	"EDGE_ENGINE_PROJECT_ID": "--edgeEngine-project-id--",
	"HZN_EXCHANGE_URL": "http://localhost:3090/v1/",
	"HZN_CSS_URL": "http://localhost:9443",
	"HZN_AGBOT_URL": "http://localhost:3111",
	"HZN_EXCHANGE_USER_AUTH": "--horizon-exchange-user-auth-(username:password)--",
	"ESS_TRACKED_OBJECT_TYPES": "--comma-separated-object-types--",
	"NODE_TLS_REJECT_UNAUTHORIZED": "0", // In order to use self signed ess certificates
	"NODE_ENV": "local"
}
